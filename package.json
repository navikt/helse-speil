    "scripts": {
        "test:eslint": "eslint src/ --ext .js --ext .jsx --ext .tsx --ext .ts",
        "test": "TZ=UTC jest --maxWorkers=3",
        "tdd": "jest --watch --maxWorkers=3",
        "validate-ts": "tsc --noEmit --project ./tsconfig.json",
        "pretest": "npm run test:eslint && npm run validate-ts",
        "start": "NODE_ENV=development parcel src/client/index.html",
        "start-mac": "NODE_ENV=development parcel src/client/index.html --open 'Google Chrome'",
        "start-express": "npm run build-server && TZ=Europe/Oslo node dist/server/server.js --experimental-modules",
        "dev-express": "export NODE_ENV=development && export SESSION_SECRET=whatever && ts-node-dev --respawn --transpileOnly ./src/server/server.ts",
        "oppgulp": "npm run dev-express",
        "build": "npm run clean:dist && npm run test && parcel build src/client/index.html --out-dir dist/client --public-url /static",
        "build-server": "tsc src/server/server.ts --outDir dist --esModuleInterop -sourcemap",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write \"src/**/*.{jsx,js,css,less,ts,tsx}\"",
        "snyk-protect": "snyk protect",
        "e2e-test": "e2e-tests/run.sh",
        "e2e-test-local": "e2e-tests/run.sh; e2e-tests/stop.sh",
        "spesialist-mock": "ts-node-dev spesialistMock/mockServer.ts",
        "github-pages": "NODE_ENV=github_pages && parcel build src/client/index.html --out-dir pages --public-url ./"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,json,md}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off"
        ]
    },
