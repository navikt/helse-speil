{
    "name": "helse-speil-frontend",
    "version": "2.0.0",
    "private": true,
    "author": "Team BÃ¸mlo",
    "description": "Innsyn i saksbehandling av sykepenger",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/helse-speil.git"
    },
    "type": "module",
    "license": "MIT",
    "engines": {
        "node": "24",
        "pnpm": ">=10"
    },
    "packageManager": "pnpm@10.28.2",
    "scripts": {
        "dev": "NEXT_PUBLIC_RUNTIME_ENV=mock PORT=1234 next dev --turbopack | pino-pretty --messageKey=message",
        "lokal-spesialist": "NEXT_PUBLIC_RUNTIME_ENV=lokal PORT=1234 next dev --turbopack | pino-pretty --messageKey=message",
        "import-testperson": "dev/importer-testdata.sh",
        "prebuild": "next telemetry disable",
        "preinstall": "npx only-allow pnpm",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --ext=ts,tsx src",
        "lint:fix": "eslint --ext=ts,tsx src --fix",
        "test": "vitest run",
        "test:watch": "vitest",
        "tsc": "tsc",
        "generate-rest": "orval",
        "generate-graphql": "BEARER_TOKEN=$(curl http://localhost:8080/local-token) graphql-codegen --config graphql-codegen.yml",
        "prepare": "husky",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{js,css,md}": [
            "prettier --write --ignore-pattern 'src/io/rest/generated/**"
        ],
        "*.json": [
            "prettier --write --ignore-pattern 'src/spesialist-mock/data/personer/**'"
        ],
        "*.{ts,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.13.1",
        "@grafana/faro-web-sdk": "^2.2.1",
