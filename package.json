    "scripts": {
        "test:eslint": "eslint src/ --ext .js --ext .jsx",
        "test": "jest",
        "tdd": "jest --watch",
        "pretest": "npm run test:eslint",
        "start": "NODE_ENV=development parcel src/client/index.html",
        "start-no-browser": "NODE_ENV=development parcel src/client/index.html",
        "start-mac": "NODE_ENV=development parcel src/client/index.html --open 'Google Chrome'",
        "start-express": "npm run build-server && TZ=Europe/Oslo node dist/server/server.js --experimental-modules",
        "dev-express": "export NODE_ENV=development && export SESSION_SECRET=whatever && npm run start-express",
        "oppgulp": "nodemon --watch src/server --exec 'npm run dev-express'",
        "build": "npm run clean:dist && npm run test && parcel build src/client/index.html --out-dir dist/client --public-url /static --no-source-maps",
        "build-server": "$(npm bin)/babel --config-file ./babel-server.config.json src/server --out-dir dist/server",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write \"src/**/*.{jsx,js,css}\"",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect",
        "e2e-test": "e2e-tests/run.sh"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,css,json,md}": [
            "prettier --write",
            "git add"
        ],
        "*.{js,jsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off",
            "git add"
        ]
    },
