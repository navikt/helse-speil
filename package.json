        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-hook-form": "^7.54.2",
        "remeda": "^2.21.0",
        "tailwind-merge": "^3.4.0",
        "use-debounce": "^10.0.5",
        "uuid": "^11.1.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "@graphiql/toolkit": "^0.11.3",
        "@graphql-codegen/cli": "^5.0.5",
