        "nav-frontend-typografi-style": "^1.0.13",
        "openid-client": "^3.4.0",
        "prom-client": "^11.5.3",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-modal": "^3.9.1",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
