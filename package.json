{
    "name": "helse-speil",
    "version": "1.0.0",
    "main": "src/index.js",
    "author": "Jan-KÃ¥re Solbakken <jan-kare.solbakken@nav.no>",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/helse-speil.git"
    },
    "license": "MIT",
    "scripts": {
        "test:eslint": "eslint src/ --ext .js --ext .jsx",
        "test": "jest",
        "tdd": "jest --watch",
        "pretest": "npm run test:eslint",
        "start": "NODE_ENV=development parcel src/index.html --open",
        "start-mac": "NODE_ENV=development parcel src/index.html --open 'Google Chrome'",
        "start-express": "npm run build-server && DEBUG=express:* node dist/server/server.js --experimental-modules",
        "dev-express": "npm run build-server && DEBUG=express:* NODE_ENV=development node dist/server/server.js --experimental-modules",
        "oppgulp": "nodemon --watch src/server --exec 'npm run dev-express'",
        "mock-s3": "docker-compose --file __mocks__/s3-docker-compose.yaml up",
        "build": "npm run clean:dist && npm run test && parcel build src/index.html --out-dir dist/client --public-url ./ --no-source-maps",
        "build-server": "$(npm bin)/babel --config-file ./babel-server.config.json src/server --out-dir dist/server",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write \"src/**/*.{jsx,js,css}\"",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,css,json,md}": [
            "prettier --write",
            "git add"
        ],
        "*.{js,jsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "aws-sdk": "^2.518.0",
