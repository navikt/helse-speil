        "@navikt/ds-tailwind": "^8.2.2",
        "@navikt/next-logger": "^4.5.1",
        "@navikt/oasis": "^4.1.1",
        "@portabletext/react": "^3.2.1",
        "@tanstack/react-query": "^5.90.7",
        "@types/uuid": "^10.0.0",
        "axios": "^1.13.5",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.13",
        "graphql": "^16.10.0",
        "graphql-yoga": "^5.12.1",
        "jose": "^6.0.8",
        "jotai": "^2.16.2",
        "motion": "^12.4.7",
        "next": "^16.1.5",
        "next-themes": "^0.4.6",
        "nextleton": "^0.6.1",
        "pino": "^10.3.0",
        "pino-socket": "^7.4.0",
        "prom-client": "^15.1.3",
