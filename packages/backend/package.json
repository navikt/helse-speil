{
    "name": "helse-speil-backend",
    "version": "1.0.0",
    "main": "server.ts",
    "author": "Team BÃ¸mlo",
    "description": "Innsyn i saksbehandling av sykepenger",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/helse-speil.git"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "start": "npm run build && TZ=Europe/Oslo node dist/server/server.js --experimental-modules",
        "dev": "cross-env-shell NODE_ENV=development SESSION_SECRET=whatever ts-node-dev --respawn --transpile-only server.ts",
        "serve": "cross-env-shell NODE_ENV=development SESSION_SECRET=whatever ts-node-dev --respawn --transpile-only server.ts",
        "mock": "ts-node-dev spesialistMock/server.ts",
        "test:eslint": "eslint . --ext .ts --ext .js",
        "test": "cross-env TZ=UTC jest",
        "test-ci": "npm run test -- --maxWorkers=50% --cacheDirectory=$jest_cache_dir",
        "clean": "rm -rf node_modules",
        "tdd": "TZ=UTC jest --watch",
        "validate-ts": "npx tsc --noEmit --project tsconfig.json",
        "pretest": "npm run test:eslint && npm run validate-ts",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write \"**/*.{js,ts}\""
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "compression": "^1.7.4",
        "connect-redis": "^5.1.0",
        "cookie-parser": "^1.4.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.7",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "nanoid": "^3.1.31",
        "openid-client": "^5.4.2",
        "prom-client": "^13.1.0",
        "redis": "^3.1.0",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "snyk": "^1.733.0",
        "tunnel": "0.0.6",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/polyfill": "^7.12.1",
        "@graphql-tools/schema": "^8.3.1",
        "@graphql-tools/utils": "^8.6.1",
        "@trivago/prettier-plugin-sort-imports": "^2.0.2",
        "@types/compression": "^1.7.0",
        "@types/connect-redis": "^0.0.16",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/jest": "^27.0.1",
        "@types/redis": "^2.8.28",
        "@types/request-promise-native": "^1.0.17",
        "@types/tunnel": "0.0.2",
        "@typescript-eslint/eslint-plugin": "^4.21.0",
        "@typescript-eslint/parser": "^4.21.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.2.0",
        "eslint-plugin-import": "^2.23.3",
        "express-graphql": "^0.12.0",
        "graphql": "^15.8.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.4",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": ">=14.16.0",
        "npm": ">=6.14.0"
    },
    "snyk": true
}
