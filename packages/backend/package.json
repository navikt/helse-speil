{
    "name": "helse-speil-backend",
    "version": "1.0.0",
    "main": "server.ts",
    "author": "Team BÃ¸mlo",
    "description": "Innsyn i saksbehandling av sykepenger",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/helse-speil.git"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && TZ=Europe/Oslo node dist/server/server.js --experimental-modules",
        "dev": "cross-env-shell NODE_ENV=development ts-node-dev --respawn --transpile-only server.ts",
        "serve": "cross-env-shell NODE_ENV=development ts-node-dev --respawn --transpile-only server.ts",
        "lint": "eslint . --ext .ts --ext .js",
        "test": "cross-env TZ=UTC jest",
        "test-ci": "npm run test -- --maxWorkers=50% --cacheDirectory=$jest_cache_dir",
        "clean": "rimraf node_modules",
        "tdd": "TZ=UTC jest --watch",
        "validate-ts": "tsc",
        "pretest": "npm run lint && npm run validate-ts",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write \"**/*.{js,ts}\""
    },
    "dependencies": {
        "@navikt/oasis": "^3.3.0",
        "babel-polyfill": "^6.26.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "http-proxy": "^1.18.1",
        "http-proxy-middleware": "^3.0.0",
        "prom-client": "^15.1.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.6",
        "@babel/plugin-transform-runtime": "^7.23.6",
        "@babel/polyfill": "^7.12.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/http-proxy": "^1.17.14",
        "@types/jest": "^29.5.11",
        "@types/tunnel": "0.0.6",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^29.7.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.9.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0"
    },
    "engines": {
        "node": ">=14.17.0",
        "npm": ">=6.14.0"
    }
}
