  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "npm run build && TZ=Europe/Oslo node dist/server/server.js --experimental-modules",
    "dev-backend": "cross-env-shell NODE_ENV=development SESSION_SECRET=whatever ts-node-dev --respawn --transpile-only server.ts",
    "dev-mock": "ts-node-dev spesialistMock.ts",
    "test:eslint": "eslint src/ --ext .ts --ext .js",
    "test": "cross-env TZ=UTC jest --maxWorkers=3",
    "tdd": "TZ=UTC jest --watch --maxWorkers=3",
    "validate-ts": "tsc --noEmit --project tsconfig.json",
    "pretest": "npm run test:eslint && npm run validate-ts",
    "clean:dist": "rimraf dist",
    "prettier": "prettier --write \"**/*.{js,ts}\""
  },
  "dependencies": {
